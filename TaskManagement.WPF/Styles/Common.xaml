<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TaskManagement.WPF.Styles">
    <Color x:Key="BaseColor1">#808080</Color>
    <Color x:Key="BaseColor2">#FFD700</Color>

    <SolidColorBrush x:Key="BaseBrush1" Color="{StaticResource BaseColor1}"/>
    <SolidColorBrush x:Key="BaseBrush2" Color="{StaticResource BaseColor2}"/>
</ResourceDictionary>